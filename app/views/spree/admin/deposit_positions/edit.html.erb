<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @deposit_position } %>

<% content_for :page_title do %>
  <%= link_to Spree.t(:deposit), admin_product_variant_deposit_path(@deposit_position.deposit.variant.product, @deposit_position.deposit.variant) %> /
  <%= @deposit_position.name %>
<% end %>

<%= form_for([:admin, @deposit_position]) do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>